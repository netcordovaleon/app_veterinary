{
  "name": "self_service_app",
  "version": "0.0.1",
  "author": "Ionic Framework",
  "homepage": "http://ionicframework.com/",
  "private": true,
  "scripts": {
    "commitmsg": "commitlint -e $GIT_PARAMS",
    "eslint:fix": "eslint --fix",
    "eslint:fix:all": "eslint --fix ./src/**/**/*.ts",
    "start": "ionic-app-scripts serve",
    "clean": "ionic-app-scripts clean",
    "build": "ionic-app-scripts build",
    "lint": "ionic-app-scripts lint",
    "sasslint": "sass-lint --format --verbose --no-exit",
    "sasslint:all": "sass-lint ./src/**/*.scss --verbose --no-exit",
    "stylelint:fix": "stylelint --syntax=scss --fix",
    "stylelint:fix:all": "stylelint ./src/**/**/*.scss --fix",
    "tslinter:fix": "tslint --format verbose --project tsconfig.json --config tslint.json -t stylish --fix"
  },
  "dependencies": {
    "@angular/animations": "5.2.11",
    "@angular/common": "5.2.11",
    "@angular/compiler": "5.2.11",
    "@angular/compiler-cli": "5.2.11",
    "@angular/core": "5.2.11",
    "@angular/forms": "5.2.11",
    "@angular/http": "5.2.11",
    "@angular/platform-browser": "5.2.11",
    "@angular/platform-browser-dynamic": "5.2.11",
    "@ionic-native/core": "~4.12.0",
    "@ionic-native/splash-screen": "~4.12.0",
    "@ionic-native/status-bar": "~4.12.0",
    "@ionic/pro": "2.0.3",
    "@ionic/storage": "2.1.3",
    "cordova-android": "7.0.0",
    "cordova-ios": "4.5.5",
    "cordova-plugin-device": "^2.0.2",
    "cordova-plugin-ionic-keyboard": "^2.1.3",
    "cordova-plugin-ionic-webview": "^2.1.4",
    "cordova-plugin-splashscreen": "^5.0.2",
    "cordova-plugin-whitelist": "^1.3.3",
    "ionic-angular": "3.9.2",
    "ionicons": "3.0.0",
    "ramda": "^0.25.0",
    "rxjs": "5.5.11",
    "sw-toolbox": "3.6.0",
    "zone.js": "0.8.26"
  },
  "devDependencies": {
    "@commitlint/cli": "^6.0.2",
    "@commitlint/config-conventional": "^6.0.2",
    "@ionic/app-scripts": "^3.2.0",
    "eslint": "^4.16.0",
    "eslint-config-prettier": "^2.9.0",
    "eslint-plugin-prettier": "^2.5.0",
    "lint-staged": "^7.1.0",
    "sass-lint": "^1.12.1",
    "stylelint": "^9.2.0",
    "stylelint-config-recommended-scss": "^3.0.0",
    "stylelint-config-standard": "^18.0.0",
    "stylelint-scss": "^3.1.0",
    "tslint-ionic-rules": "0.0.17",
    "typescript": "~2.6.2",
    "typescript-eslint-parser": "^15.0.0"
  },
  "description": "An Ionic project",
  "lint-staged": {
    "*.{ts,tsx, js}": [
      "npm run prettier:ts",
      "npm run tslinter:fix",
      "npm run ionic:build",
      "git add"
    ],
    "*.scss": [
      "npm run prettier:scss",
      "npm run stylelint:fix",
      "npm run sasslint",
      "git add"
    ]
  },
  "cordova": {
    "plugins": {
      "cordova-plugin-whitelist": {},
      "cordova-plugin-device": {},
      "cordova-plugin-splashscreen": {},
      "cordova-plugin-ionic-webview": {
        "ANDROID_SUPPORT_ANNOTATIONS_VERSION": "27.+"
      },
      "cordova-plugin-ionic-keyboard": {}
    },
    "platforms": [
      "ios",
      "android"
    ]
  }
}
